        "packages/lib-27/*",
        "packages/lib-28/*",
        "packages/lib-29/*",
        "packages/lib-30/*",
        "packages/lib-31/*",
        "packages/lib-32/*",
        "packages/lib-33/*",
        "packages/lib-34/*",
        "packages/lib-35/*",
        "packages/lib-36/*",
        "packages/lib-37/*",
        "packages/lib-38/*",
        "packages/lib-39/*",
        "packages/lib-40/*",
        "packages/lib-41/*",
        "packages/lib-42/*",
        "packages/lib-43/*",
        "packages/lib-44/*",
        "packages/lib-45/*",
        "packages/lib-46/*",
        "packages/lib-47/*",
        "packages/lib-48/*",
        "packages/lib-49/*",
        "packages/lib-50/*",
        "packages/lib-51/*",
        "packages/lib-52/*",
        "packages/lib-53/*",
        "packages/lib-54/*",
        "packages/lib-55/*",
        "packages/lib-56/*",
        "packages/lib-57/*",
        "packages/lib-58/*",
        "packages/lib-59/*",
        "packages/lib-60/*",
