{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "out-tsc/demo",
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "jsx": "react-jsx",
    "lib": [
      "dom"
    ],
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts",
      "vite/client"
    ],
    "rootDir": "src",
    "module": "esnext",
    "moduleResolution": "bundler"
